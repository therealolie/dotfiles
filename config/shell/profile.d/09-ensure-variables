#!/bin/sh
[ -z "${_DISPLAYS}" ] && _DISPLAYS=0
[ -z "${EMOJI}" ] && EMOJI='ðŸŒ¸'
export EMOJI
[ -z "${COLORS}" ] && COLORS=1
export COLORS
_DISPLAYS=0
export _DISPLAYS
_x=0
while [ "${_x}" -lt "${COLORS}" ]; do
	eval "_cur=\"\${COL${_x}}\""
	[ -z "${_cur}" ] && eval "COL${_x}=\"7\""
	eval "export COL${_x}"
	_x=$(( _x + 1 )) 
done
unset _x
unset _cur
